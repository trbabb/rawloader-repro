{
  "exclude": ["node_modules"],
  "compilerOptions": {
       "target": "ES2018",
       "module": "es2015",
       "declaration": true,
       "sourceMap": true,
       "outDir": "built",
       "rootDir": "./",
       "composite": true,
       "strict": true,
       "esModuleInterop": true,
       "forceConsistentCasingInFileNames": true
  }
}
